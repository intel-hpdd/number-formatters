    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-prettier": "2.0.1",
    "flow-bin": "0.44.2",
    "intel-flow-jasmine": "1.6.0",
    "intel-jasmine-n-matchers": "2.1.1",
