{
  "name": "@mfl/number-formatters",
  "version": "1.0.1",
  "description": "Various number formatting / conversion modules.",
  "main": "source/index.js",
  "scripts": {
    "test": "jest",
    "cover": "NODE_ENV=test npm t -- --coverage",
    "flow": "flow",
    "eslint": "eslint ./"
  },
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": [
    "flow",
    "eslint",
    "test"
